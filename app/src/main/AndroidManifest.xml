<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caferegister">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/main_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CafeRegister">
        <activity
            android:name=".DonutOrderActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DonutActivity"
            android:exported="false"
            android:label="@string/activity_donut_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CoffeeActivity"
            android:exported="false"
            android:label="@string/activity_coffee_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StoreOrdersActivity"
            android:exported="false"
            android:label="@string/activity_order_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".YourOrderActivity"
            android:exported="false"
            android:label="@string/activity_store_orders_name"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>